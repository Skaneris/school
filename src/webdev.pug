extends template/tpl

block content
    - var text = ['Learn how to be a great web developer and build full-stack apps, with an advanced understanding of JavaScript.']
    +pageHeader('Web Development Immersive Courses', text, 'webdev.jpg')
        ul 
            li 
                span.icon(style="background-image: url(images/svg/icons/calendar.svg)")
                span.text 8 WEEKS, FULL-TIME
            li 
                span.icon(style="background-image: url(images/svg/icons/location.svg)")
                span.text ON SITE OR REMOTE
            li 
                span.icon(style="background-image: url(images/svg/icons/bag.svg)")
                span.text 99% HIRING RATE
            li 
                span.icon(style="background-image: url(images/svg/icons/chart.svg)")
                span.text +25% HIGHER AVG SALARY
        button.button(data-action="order") Apply Today